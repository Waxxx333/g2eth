#compdef g2eth

_arguments -s \
    '(-g --gpu)'{-g,--gpu}'[GPU to get stats on]' \
    '(-l --list)'{-l,--list}'[List all GPUs capable of mining]' \
    '(-u --usage)'{-u,--usage}'[More detailed help]' \
    '(-p --price)'{-p,--price}'[Get current price of ETH]' \
    '(-c --convert)'{-c,--convert}'[Convert ETH or currency(USD/GBP/CAD/EUR)]' \
    '(-i --into)'{-i,--into}'[Converting into(USD/GBP/CAD/EUR]' \
    '(-n --amount)'{-n,--amount}'[Amount to convert]' \
    '(-h --help)'{-h,--help}'[Display this help and exit]' \
    '(-v --version)'{-v,--version}'[Output version information and exit]'
